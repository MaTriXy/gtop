{
	// obtain these from the Google API Console
	"client_id": "",
	"client_secret": "",
	"redirect_dir": "https://example.com/gtop/oauth2callback",

	// the same info in the Google API that can be used for sending subscription pushes to
	"subscription_callback": "https://example.com/subscription",

	// the name of the host. Purely for display usage
	"hostname": "someserver.com",

	// the icon for the contact
	"contactIcon": "http://jaxbot.me/glass/gtop/gtop.jpg",
	"displayName": "gtop",
	"speakableName": "G Top",

	// the port the http server should listen on
	"port": 8101,

	// thresholds for the colors of the gtop data
	"midMemPercentage": 30,
	"highMemPercentage": 50,
	"midCpuLoad": 3,
	"highCpuLoad": 5,

	// an id to identify the card provider. Important for you have multiple gtop servers
	"id": "jaxbot.me.test",

	// in minutes
	"updateFrequency": 5,

	"commands": [
		{ "aliases": [
			"hello"
				],
			"command": "echo hi",
			"sendback": true
		}
	]	
}
